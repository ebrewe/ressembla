{
  "manifest_version": 2,
  "name": "Ressembla",
  "description": "This extension fixes Assembla",
  "version": "0.0.1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "content_scripts":[
    {
      "matches": ["https://www.assembla.com/*"],
      "js": ["jquery.min.js", "bundle.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "https://www.assembla.com/"
  ]
}
